{
    "git.ignoreLimitWarning": true,
    "editor.renderWhitespace": "all",
    "files.insertFinalNewline": true,
    "editor.renderIndentGuides": false,
    "editor.minimap.enabled": false,
    "editor.trimAutoWhitespace": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/dist": true,
        "**/node_modules": true
    }
}
